#!/bin/bash
N_NODES=(5 7)
CON_EPOCH=25
IMP_EPOCH=40
# for filename in ./runs/configFiles/Transformer-*; do
for problem_size in "${N_NODES[@]}"; do
	for filename in runs/configFiles/config-PointerNetwork-*; do
		tsp python3 Main.py $filename $CON_EPOCH $problem_size
	done
	for filename in runs/configFiles/config-Transformer-*; do
		tsp python3 Main.py $filename $CON_EPOCH $problem_size
	done
	for filename in runs/configFiles/config-TSP_improve-*; do
		tsp python3 Main.py $filename $IMP_EPOCH $problem_size
	done
done
