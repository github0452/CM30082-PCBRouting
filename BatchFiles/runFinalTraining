	# 	tsp python3 Main.py $filename $CON_EPOCH $problem_size
	# done
	for filename in finalTraining/config/Transformer-*; do
		tsp python3 Main.py $filename $CON_EPOCH $problem_size True
	done
	for filename in finalTraining/config/TSP_improve-*; do
		tsp python3 Main.py $filename $IMP_EPOCH $problem_size True
	done
